{
  "routes": [
    {
      "path": "/api/posts",
      "method": "POST",
      "fields": ["title"],
      "action": "both",
      "check_type": ["US_SSN", "CREDIT_CARD"]
    },
    {
      "path": "/api/posts",
      "method": "POST",
      "fields": ["caption"],
      "action": "both",
      "check_type": ["US_SSN", "CREDIT_CARD"]
    },
    {
      "path": "/api/posts/:postId/comment",
      "method": "POST",
      "fields": ["text"],
      "action": "both",
      "check_type": ["US_SSN", "CREDIT_CARD"]
    },
    {
      "path": "/api/friends/messages",
      "method": "POST",
      "fields": ["text"],
      "action": "warn",
      "check_type": ["US_SSN", "CREDIT_CARD", "IBAN_CODE", "US_PASSPORT", "EMAIL_ADDRESS", "DATE_TIME_DOB"]
    }
  ],
  "piiCheckerUrl": "http://localhost:12423/anonymize",
  "timeout": 5000,
  "availableEntityTypes": {
    "custom": [
      "DATE_TIME_DOB",
      "ADDRESS"
    ],
    "presidio_global": [
      "CREDIT_CARD",
      "CRYPTO",
      "EMAIL_ADDRESS",
      "IBAN_CODE",
      "IP_ADDRESS",
      "NRP",
      "PHONE_NUMBER",
      "MEDICAL_LICENSE"
    ],
    "presidio_usa": [
      "US_BANK_NUMBER",
      "US_DRIVER_LICENSE",
      "US_ITIN",
      "US_PASSPORT",
      "US_SSN"
    ],
    "presidio_uk": [
      "UK_NHS",
      "UK_NINO"
    ],
    "presidio_spain": [
      "ES_NIF",
      "ES_NIE"
    ],
    "presidio_italy": [
      "IT_FISCAL_CODE",
      "IT_DRIVER_LICENSE",
      "IT_VAT_CODE",
      "IT_PASSPORT",
      "IT_IDENTITY_CARD"
    ],
    "presidio_poland": [
      "PL_PESEL"
    ],
    "presidio_singapore": [
      "SG_NRIC_FIN",
      "SG_UEN"
    ],
    "presidio_australia": [
      "AU_ABN",
      "AU_ACN",
      "AU_TFN",
      "AU_MEDICARE"
    ],
    "presidio_india": [
      "IN_PAN",
      "IN_AADHAAR",
      "IN_VEHICLE_REGISTRATION",
      "IN_VOTER",
      "IN_PASSPORT",
      "IN_GSTIN"
    ],
    "presidio_finland": [
      "FI_PERSONAL_IDENTITY_CODE"
    ],
    "presidio_korea": [
      "KR_RRN"
    ],
    "presidio_thai": [
      "TH_TNIN"
    ],
    "filtered_out": [
      "URL",
      "PERSON",
      "LOCATION",
      "DATE_TIME"
    ],
    "special": [
      "others"
    ],
    "notes": [
      "Entity types are returned from the API with spaces (e.g., 'DATE TIME DOB') but should be specified in config with underscores (e.g., 'DATE_TIME_DOB'). The middleware handles this conversion automatically.",
      "The following entities are filtered out in main.py: URL, PERSON, LOCATION, DATE_TIME (unless in DOB context, then converted to DATE_TIME_DOB).",
      "Entities with confidence < 0.4 are categorized as 'others'.",
      "DATE_TIME entities are only returned if they appear in DOB context (converted to DATE_TIME_DOB)."
    ]
  }
}

